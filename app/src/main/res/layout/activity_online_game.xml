<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.ActivityOnlineGame">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="329dp" />

    <ScrollView
        android:layout_width="282dp"
        android:layout_height="115dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="136dp">

    </ScrollView>

    <EditText
        android:id="@+id/enter_message_pole"
        android:layout_width="274dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/enter_message"
        tools:layout_editor_absoluteX="449dp"
        tools:layout_editor_absoluteY="252dp" />

    <Button
        android:id="@+id/chat_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chat"
        tools:layout_editor_absoluteX="633dp"
        tools:layout_editor_absoluteY="303dp" />

    <TableLayout
        android:layout_width="703dp"
        android:layout_height="85dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="678dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/player1_image"
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    tools:srcCompat="@tools:sample/avatars" />

                <Button
                    android:id="@+id/vote_for_player1_button"
                    android:layout_width="76dp"
                    android:layout_height="wrap_content"
                    android:text="@string/vote" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="78dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/player2_image"
                    android:layout_width="96dp"
                    android:layout_height="match_parent"
                    tools:srcCompat="@tools:sample/avatars" />

                <Button
                    android:id="@+id/vote_for_player2_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/vote" />

            </FrameLayout>

            <FrameLayout
                android:layout_width="76dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/player3_image"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    tools:srcCompat="@tools:sample/avatars" />

                <Button
                    android:id="@+id/vote_for_player3_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/vote" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="85dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/player4_image"
                    android:layout_width="82dp"
                    android:layout_height="match_parent"
                    tools:srcCompat="@tools:sample/avatars" />

                <Button
                    android:id="@+id/vote_for_player4_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/vote" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/player5_image"
                    android:layout_width="86dp"
                    android:layout_height="83dp"
                    tools:srcCompat="@tools:sample/avatars" />

                <Button
                    android:id="@+id/vote_for_player5_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/vote" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/player6_image"
                    android:layout_width="84dp"
                    android:layout_height="79dp"
                    tools:srcCompat="@tools:sample/avatars" />

                <Button
                    android:id="@+id/vote_for_player6_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/vote" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/player7_image"
                    android:layout_width="85dp"
                    android:layout_height="match_parent"
                    tools:srcCompat="@tools:sample/avatars" />

                <Button
                    android:id="@+id/vote_for_player7_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/vote" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/player8_image"
                    android:layout_width="98dp"
                    android:layout_height="match_parent"
                    tools:srcCompat="@tools:sample/avatars" />

                <Button
                    android:id="@+id/vote_for_player8_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/vote" />
            </FrameLayout>

        </TableRow>

    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>